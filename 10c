import matplotlib.pyplot as plt 
# Define the data 
x= [5, 7, 8, 7, 2, 17, 2, 9, 4, 11, 12, 9, 6] 
y = [99, 86, 87, 88, 111, 86, 103, 87, 94, 78, 77, 85, 86] 
# Plot the scatter graph 
plt.scatter(x, y, color='blue') 
plt.xlabel('X AXIS') 
plt.ylabel('Y AXIS') 
plt.title('Scatter Plot') 
plt.show()
